{
  "name": "tsuuka-api",
  "description": "REST API for TsÅ«ka built with Node & Express",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Theerawat Pongsupawat",
    "email": "rxseven.com@gmail.com",
    "url": "http://www.rxseven.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxseven/tsuuka-api.git"
  },
  "engines": {
    "node": "10.15.1",
    "npm": "6.4.1"
  },
  "main": "src/app.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "format:debug": "prettier --debug-check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "start": "node build/server.js",
    "start:compiler": "tsc -w",
    "start:server": "nodemon build/server.js",
    "start:watch": "concurrently \"npm run start:compiler\" \"npm run start:server\"",
    "test": "jest --config jest.config.json",
    "test:coverage": "npm test -- --coverage --no-cache --silent",
    "test:watch": "npm test -- --watch",
    "test:watch:silent": "npm test -- --silent --watch",
    "test:watch:verbose": "npm test -- --verbose --watch"
  },
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-promise-router": "3.0.3",
    "lodash": "4.17.11",
    "morgan": "1.9.1"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/body-parser": "1.17.0",
    "@types/cors": "2.8.4",
    "@types/dotenv": "6.1.0",
    "@types/eslint": "4.16.6",
    "@types/express": "4.16.1",
    "@types/express-promise-router": "2.0.1",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.122",
    "@types/morgan": "1.7.35",
    "@types/nock": "9.3.1",
    "@types/node": "11.11.0",
    "@types/prettier": "1.16.1",
    "@types/supertest": "2.0.7",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "@typescript-eslint/parser": "1.4.2",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "eslint": "5.15.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "jest": "24.3.1",
    "lint-staged": "8.1.5",
    "nock": "10.0.6",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "supertest": "4.0.0",
    "ts-jest": "24.0.0",
    "ts-node": "8.0.3",
    "typescript": "3.3.3333"
  }
}
